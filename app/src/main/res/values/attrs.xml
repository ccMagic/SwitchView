<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitchView">
        <!--选中项的背景-->
        <attr name="SwitchViewIndexBackground" format="reference" />
        <!--选中项的字体大小-->
        <attr name="SwitchViewIndexTextSize" format="dimension" />
        <!--选中项的字体颜色-->
        <attr name="SwitchViewIndexTextColor" format="color" />
        <!--未选项的背景-->
        <attr name="SwitchViewNormalBackground" format="reference" />
        <!--未选项的字体颜色-->
        <attr name="SwitchViewDefaultTextColor" format="color" />
        <!--未选项的字体大小-->
        <attr name="SwitchViewDefaultTextSize" format="dimension" />
    </declare-styleable>

</resources>